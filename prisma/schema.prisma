datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/dedeicev?schema=public"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  createdAt DateTime @default(now())
  name  String
  email String  @unique
  password String
}